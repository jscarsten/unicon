procedure printnode(n)
	write("node type: " || type(n))
	write("node label: " || n.label)
end

procedure caseexpr(root)
	write("\n------------------------------------")
	write("caseexpr.icn output :\n")

	node := root
	
	case type(node) of{
		"treenode" : {
			every child := !node.children do caseexpr(child)
			printnode(node)
		}
		"null" : fail
		"declaration__state" : fail
		"Class__state" | "argList__state" : fail
	}

	write("------------------------------------")
end

